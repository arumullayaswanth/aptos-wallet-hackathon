# Environment Variables Template
# Copy this file to .env and fill in your values

# Aptos Network Configuration
APTOS_NETWORK=testnet
APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1
APTOS_FAUCET_URL=https://faucet.testnet.aptoslabs.com

# Module Publisher Account (your account that will deploy the contract)
MODULE_PUBLISHER_ACCOUNT_ADDRESS=0x1a2b3c4d5e6f7890abcdef1234567890abcdef12
MODULE_PUBLISHER_ACCOUNT_PRIVATE_KEY=0xabcdef...

# Aptos API Configuration
APTOS_API_KEY=your_api_key_here

# Frontend Configuration
VITE_APP_TITLE=Research Data Timestamp Registry
VITE_APTOS_NETWORK=testnet
VITE_MODULE_ADDRESS=0x1a2b3c4d5e6f7890abcdef1234567890abcdef12

# Development
NODE_ENV=development
PORT=3000